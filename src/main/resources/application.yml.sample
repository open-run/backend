spring:
  datasource:
    url: jdbc:mysql://host:3306/database_name?useSSL=false
    username: ${{ DATASOURCE_USERNAME }}
    password: ${{ DATASOURCE_PASSWORD }}

  data:
    redis:
      password: ${{ REDIS_PASSWORD }}

jwt:
  secret:
    key: ${{ JWT_SECRET_KEY }}

nft:
  contract-address: ${{ CONTRACT_HASH }}
  private-key: ${{ CONTRACT_PRIVATE_KEY }}
  rpc-url: ${{ BASE_CHAIN_RPC_URL }}
  chain-id: 1660990954
  pinata-dedicated-gateway: https://${my-domain}.mypinata.cloud
